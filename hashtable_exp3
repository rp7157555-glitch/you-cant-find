#include <stdio.h> 
 
int hashtable[10]; 
 
void insert(int key) {  
int i=1; 
int index = key % 10; 
int original_index = index; 
 
while (hashtable[index] != -1) { 
index = (original_index + i) % 10; 
i++; 
 
if (i == 10) { 
printf("Hash table is full. Cannot insert %d\n", key); 
return; 
} 
} 
 
hashtable[index] = key; 
printf("Inserted %d at index %d\n", key, index); 
} 
 
void display() { 
printf("\nYour Hash Table:\n"); 
printf("Index\tKeyValue\n"); 
for (int i = 0; i < 10; i++) { 
printf("%d\t%d\n", i, hashtable[i]); 
} 
} 
int search(int key){ 
int i; 
int index=key %10; 
for(i=0;i<10;i++) 
{ 
if(hashtable[index]==key) 
{ 
return index; 
}else 
{ 
index=(index+i)%10; 
} 
} 
return -1; 
} 
 
int main() { 
int i, choice, key,search_key; 
 
// Initialize hash table 
for (i = 0; i < 10; i++) { 
hashtable[i] = -1; 
} 
 
do { 
printf("\n1. Insert\n2. Display\n3.search\n4.exit "); 
printf("\nEnter your choice: "); 
scanf("%d", &choice); 
 
switch (choice) { 
case 1: 
printf("Enter the element: "); 
scanf("%d", &key); 
insert(key); 
break; 
 
case 2: 
display(); 
break; 
 
case 3: 
printf("enter the search key"); 
scanf("%d",&key);  
int pos = search(key); 
if(pos==-1) 
{ 
printf("key not found"); 
} 
else 
{ 
printf("key found at pos %d",pos); 
} 
break; 
 
} 
} while (choice != 4); 
 
return 0; 
}
